[masters]
%{ for index, (hostname, ip) in enumerate(masters) ~}
${master-0.index+1} ansible_host=${ip} ansible_user=${user} 
%{ endfor ~}

[workers]
%{ for index, (hostname, ip) in enumerate(workers) ~}
${worker-0.index} ansible_host=${ip} ansible_user=${user}
%{ endfor ~}

[k8s_cluster:children]
masters
workers